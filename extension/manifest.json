{
    "manifest_version": 3,
    "name": "Live Traffic Visualizer",
    "version": "1.0",
    "permissions": ["activeTab", "scripting", "storage"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
    "host_permissions": ["<all_urls>"]
}
  